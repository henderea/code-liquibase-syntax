{
    "name": "liquibase",
    "displayName": "Liquibase",
    "description": "Liquibase syntax highlighting",
    "version": "1.0.0",
    "publisher": "henderea",
    "repository": {
        "type": "git",
        "url": "https://github.com/henderea/code-liquibase-syntax"
    },
    "bugs": {
        "url": "https://github.com/henderea/code-liquibase-syntax/issues"
    },
    "engines": {
        "vscode": "^1.17.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "liquibase",
            "aliases": ["Liquibase", "liquibase"],
            "extensions": [".xml"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "liquibase",
            "scopeName": "text.liquibase",
            "path": "./syntaxes/liquibase.tmLanguage.json",
            "embeddedLanguages": {
                "text.xml": "xml",
                "source.sql": "sql"
            }
        }]
    }
}